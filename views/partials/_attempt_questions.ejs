<ul class="quiz-list-item my-5">
  <br>
    <div class="card">
      <div class="card-header">
        <h3 class="card-header-title is-size-5">
          <%- question.question_text %>
        </h3>
      </div>
      <div class="card-content">
        <p class="middle">
          <% for (option of question.options) { %>
            <% if (option.option_id === question.correct_option_id) { %>
              <ul><p class="correct">&nbsp <%- option.option_text %>&nbsp&nbsp&nbsp ✅✅</p></ul><br/>
            <% } else if (option.selected_option_id !== question.correct_option_id && question.selected_option_id === option.option_id) { %>
              <ul><p class="incorrect">&nbsp <%- option.option_text %>&nbsp&nbsp&nbsp ❌❌</p></ul><br/>
            <% } else { %>
              <ul><p>&nbsp <%- option.option_text %></p></ul><br/>
            <% } %>
          <% } %>
        </p>
      </div>
    </div>
  <br>
</ul>
