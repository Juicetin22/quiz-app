<li class="quiz-list-item my-5">
  <a href="/attempts/<%- attempt.id %>">
    <div class="card">
      <div class="card-header">
        <h3 class="card-header-title is-size-5">
          <%- attempt.quiz_title %>
        </h3>
      </div>
      <div class="card-content">
        <p class="description">
          <%- attempt.quiz_description %>
        </p>
      </div>
      <footer class="card-footer">
        <div class="card-footer-item">
          Date completed:&nbsp&nbsp
          <span class="mr-3">
            <%- attempt.time %>
          </span>
        </div>
        <div class="card-footer-item">
          Score:&nbsp&nbsp
          <span class="tag is-primary is-medium has-text-weight-semibold">
            <%- attempt.score %> %
          </span>
        </div>
    </div>
    </footer>
    </div>
  </a>
</li>
