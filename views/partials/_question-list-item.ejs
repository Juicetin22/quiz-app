<li class="card q-card mt-6">
  <div class="card-header">
    <h3 class="card-header-title is-size-5">
      <%= question.question %>
    </h3>
  </div>
  <div class="card-content">
    <ul class="control">
      <% for (option of question.options) { %>
        <li>
          <label class="radio">
            <input type="radio" name="<%=  question.question_id %>">
            <%= option.option %>
          </label>
        </li>
        <% } %>
    </ul>

  </div>
</li>
