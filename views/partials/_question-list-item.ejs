<li class="quiz-list-item my-5">
    <a href="/quizzes/<%- quiz.quiz_identifier %>">
      <div class="card">
        <div class="card-header">
          <h3 class="card-header-title is-size-5">
            <%- question.question %>
          </h3>
        </div>
        <footer class="card-footer">
          <div class="card-footer-item">
            <span class="mx-2">Questions: </span>
            <span class="tag is-primary is-medium has-text-weight-semibold">
              <%- quiz.questions %>
            </span>
          </div>
          <div class="card-footer-item">
            <span class="mx-2">Average Score: </span>
            <span class="tag is-primary is-medium has-text-weight-semibold">
              <%- quiz.avg_score %>
            </span>
          </div>
        </footer>
      </div>
    </a>
  </li>
  